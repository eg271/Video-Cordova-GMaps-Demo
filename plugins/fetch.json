{
    "cordova-plugin-crosswalk-webview": {
        "source": {
            "type": "registry",
            "id": "cordova-plugin-crosswalk-webview@2.1.0"
        },
        "is_top_level": true,
        "variables": {
            "XWALK_VERSION": "21+",
            "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
            "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
            "XWALK_MODE": "embedded",
            "XWALK_MULTIPLEAPK": "true"
        }
    },
    "cordova-plugin-googlemaps": {
        "source": {
            "type": "git",
            "url": "https://github.com/phonegap-googlemaps-plugin/cordova-plugin-googlemaps.git",
            "subdir": "."
        },
        "is_top_level": true,
        "variables": {
            "API_KEY_FOR_ANDROID": "xxxx"
        }
    }
}